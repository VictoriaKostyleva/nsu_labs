<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.vikacech.tic_tac_toe.MainActivity">



    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"


        android:id="@+id/table"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center">


        <TableRow>
            <Button android:layout_height="100dp" android:layout_width="100dp" android:id="@+id/button00"/>
            <Button android:layout_height="100dp" android:layout_width="100dp" android:id="@+id/button01"/>
            <Button android:layout_height="100dp" android:layout_width="100dp" android:id="@+id/button02"/>
        </TableRow>
        <TableRow>
            <Button android:layout_height="100dp" android:layout_width="100dp" android:id="@+id/button10"/>
            <Button android:layout_height="100dp" android:layout_width="100dp" android:id="@+id/button11"/>
            <Button android:layout_height="100dp" android:layout_width="100dp" android:id="@+id/button12"/>
        </TableRow>
        <TableRow>
            <Button android:layout_height="100dp" android:layout_width="100dp" android:id="@+id/button20"/>
            <Button android:layout_height="100dp" android:layout_width="100dp" android:id="@+id/button21"/>
            <Button android:layout_height="100dp" android:layout_width="100dp" android:id="@+id/button22"/>

        </TableRow>


    </TableLayout>



</android.support.constraint.ConstraintLayout>
